{
    "source": {
        "includePattern": "\\.(vue|js)$"
    },
    "opts": {
        "destination": "docs",
        "readme": "README.md",
        "recurse": true,
        "template": "node_modules/minami"
    },
    "plugins": [
        "node_modules/jsdoc-vuejs",
        "plugins/markdown"
    ]
}